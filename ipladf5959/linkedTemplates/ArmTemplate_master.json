{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ipladf5959"
		},
		"lsipl_AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsipl_AzureDataLakeStorage1'"
		},
		"ls_HttpServer1_gitipl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/indrang21/ADF_project01/raw/main/"
		},
		"lsipl_AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://iplproject2121.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ipladf5959_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsipl_AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('lsipl_AzureDataLakeStorage1_accountKey')]"
					},
					"ls_HttpServer1_gitipl_properties_typeProperties_url": {
						"value": "[parameters('ls_HttpServer1_gitipl_properties_typeProperties_url')]"
					},
					"lsipl_AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('lsipl_AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}